{
  "manifest_version": 3,
  "name": "CSS Switcher",
  "version": "1.0",
  "description": "Aplica folhas de estilo diferentes no site https://chat.openai.com/chat",
  "permissions": [ "scripting", "tabs" ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["foreground.js"]
    }
  ],
  "host_permissions": [ "https://chat.openai.com/*" ]



}

