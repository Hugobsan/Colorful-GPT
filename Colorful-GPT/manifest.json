{
  "manifest_version": 3,
  "name": "CSS Switcher",
  "version": "1.0",
  "description": "Aplica folhas de estilo diferentes no site https://chat.openai.com/chat",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/chat"],
      "css": ["teste1.css"],
      "js": ["popup.js"]
    }
  ]
}